    "react-resizable-panels": "^3.0.5",
    "react-window": "^2.2.3",
    "react-window-infinite-loader": "^2.0.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "simple-git": "^3.27.0",
    "socket.io": "^4.8.1",
