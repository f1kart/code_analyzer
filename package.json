{
  "name": "gemini-code-ide",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "main": "main.js",
  "scripts": {
    "start": "concurrently \"npm run start:auth\" \"npm run start:backend\" \"npm run dev\"",
    "start:auth": "node src/auth/auth-server.js",
    "start:backend": "tsx src/backend/server.ts",
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest run --config vitest.config.ts",
    "test:frontend": "vitest run --config vitest.frontend.config.ts",
    "test:backend": "vitest run --config vitest.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:coverage": "vitest run --coverage --config vitest.frontend.config.ts",
    "test:watch": "vitest --config vitest.frontend.config.ts",
    "analyze:bundle": "vite build --mode analyze",
    "analyze:bundle:serve": "vite build --mode analyze && npx serve -s dist",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "desktop": "concurrently \"npm run dev\" \"electron .\"",
    "electron:build": "electron-builder",
    "format:all": "prettier --write ."
  },
  "dependencies": {
    "@google/genai": "^1.17.0",
    "@google/generative-ai": "^0.24.1",
